{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-var": [
      "error"
    ],
    "eqeqeq": [
      "error",
      "smart"
    ],
    "complexity": [
      "error",
      {
        "max": 5
      }
    ],
    "max-depth": [
      "error",
      4
    ],
    "max-len": [
      "warn",
      {
        "code": 80,
        "tabWidth": 4,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "max-params": [
      "error",
      7
    ],
    "no-lone-blocks": "error",
    "no-new-func": "error",
    "no-new-wrappers": "error",
    "no-new": "error",
    "no-param-reassign": "error",
    "no-redeclare": "error",
    "no-loop-func": "error",
    "no-multi-spaces": "error",
    "no-useless-concat": "error",
    "no-useless-return": "error",
    "no-return-assign": "error",
    "no-self-assign": [
      "error",
      {
        "props": true
      }
    ],
    "no-sequences": "error",
    "no-throw-literal": "error",
    "no-void": "error",
    "yoda": "error",
    "no-use-before-define": "error",
    "dot-location": [
      "error",
      "property"
    ],
    "dot-notation": "error",
    "no-eval": "error",
    "no-invalid-this": "error",
    "no-magic-numbers": [
      "warn",
      {
        "ignoreArrayIndexes": true,
        "enforceConst": true,
        "detectObjects": false
      }
    ],
    "array-bracket-spacing": [
      "error",
      "always"
    ],
    "camelcase": [
      "error",
      {
        "properties": "always"
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "eol-last": [
      "error",
      "never"
    ],
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict"
      }
    ],
    "keyword-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "multiline-ternary": [
      "warn",
      "always"
    ],
    "newline-after-var": [
      "error",
      "always"
    ],
    "new-cap": "error",
    "newline-before-return": "error",
    "newline-per-chained-call": "error",
    "new-parens": "error",
    "no-multiple-empty-lines": "error",
    "no-new-object": "error",
    "object-curly-newline": [
      "error",
      {
        "multiline": true,
        "minProperties": 1
      }
    ],
    "object-curly-spacing": [
      "error",
      "always",
      {
        "arraysInObjects": true,
        "objectsInObjects": true
      }
    ],
    "object-property-newline": "error",
    "block-spacing": "error",
    "one-var-declaration-per-line": [
      "error",
      "always"
    ],
    "semi-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "space-before-blocks": "error",
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "ignore",
      "asyncArrow": "always"
    }],
    "space-in-parens": [
      "error",
      "never"
    ],
    "space-infix-ops": "error",
    "spaced-comment": [
      "warn",
      "always"
    ],
    "unicode-bom": [
      "error",
      "never"
    ],
    "no-dupe-class-members": "error",
    "no-duplicate-imports": "error",
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "no-useless-constructor": "error",
    "no-useless-rename": "error",
    "prefer-arrow-callback": "error",
    "prefer-const": "error",
    "prefer-numeric-literals": "error",
    "prefer-rest-params": "error",
    "prefer-spread": "error",
    "prefer-template": "error",
    "no-confusing-arrow": [
      "error",
      {
        "allowParens": true
      }
    ],
    "no-constant-condition": "error",
    "no-extra-parens": "error",
    "no-extra-semi": "error"
  }
}